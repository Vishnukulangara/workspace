<%= form_for :company , url: company_path(@company), method: 'patch', :html => { :multipart => true} do |f|%>
	Name:<br/>
	<%= f.text_field :name%><br/>
	Website:<br/>
	<%= f.text_field :website %><br/>
	Address:<br/>
	<%= f.text_field :address%><br/>
	Logo:<br/>
	<%= f.file_field :logo %><br/>
	<%= f.submit :class => "btn btn-default"%><br/>
<% end %><br/>
<%= link_to 'back', companies_path, class: "btn btn-default" %>
