<h2>Edit User</h2>

<%= form_for(:user, url: admin_user_path, method: 'patch') do |f| %>
  

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true %>
  </div>

  

  <br />
  <div class="actions">
    <%= f.submit :class=> "btn btn-info"  %>
  </div>
<% end %>
<br />
<%= link_to "Back", admin_users_path , class: "btn btn-default"%>