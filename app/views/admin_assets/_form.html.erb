<%= form_for @admin_asset, url: admin_assets_path do |f| %>
	<p>
		<%= f.label :name %>
		<%= f.text_field :name %>
	</p>
	<p>
		<%= f.label :asset_category %>
		<%= f.text_field :asset_category %>
	</p> 
	<p>
		<%= f.label :asset_type %>
		<%= f.text_field :asset_type %>
	</p>
	<p>
		<%= f.label :specification %>
		<%= f.text_field :specification %>
	</p>
	<p>
		<%= f.submit %>

	</p>
<% end %>
<%= link_to 'Back', admin_assets_path %>