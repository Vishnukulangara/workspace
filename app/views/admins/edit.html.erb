<h2>Edit Admin</h2>

<%= form_for(:user, url: admin_path, method: 'patch') do |f| %>
  

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true %>
  </div>

  <div class="actions">
    <%= f.submit  %>
  </div>
<% end %>
<%= link_to 'Back', admins_path %>